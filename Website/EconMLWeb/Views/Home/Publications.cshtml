    <script type="text/template" id="tmpl-publications-accordion">
        <h4 class="js-accordion__header">{%- accordionTitle %}</h4>
        <div class="js-accordion__panel"><div class="ms-row">{%- accordionPanel %}</div></div>
    </script>
    <script type="text/template" id="tmpl-publications-accordion-item-list-default">
        <article class="publication -js-publication -{%- type %}">
            <div class="publication-thumbnail-container {%- thumbWrapClass %}">
                <div class="publication-thumbnail-inner">
                    <img class="publication-thumbnail" src="{%- thumbnail %}" />
                </div>
            </div>
            <div class="content {%- thumbContentClass %}">
                <h5>
                    <a href="{%- permalink %}">
                        {%- title %}
                    </a>
                </h5>

                <span class="associated-people">
                    {%- people %}
                </span>

                <div class="book-data">
                    <span class="book-data">{%- bookData %}</span>
                </div>

                <div class="journal-data">
                    <span class="journal-data-data">
                        <em>{%- journalData %}</em>
                    </span>

                    <span class="issue-data">{%- issueData %}</span>
                </div>

                <div class="published-data">
                    <span class="published-data">{%- publishedData %}</span>
                </div>

                <div class="editors-data">
                    <em>{%- editors %}</em>
                </div>

                <div class="how-published-data">
                    <span class="how-published-data">{%- howPublished %}</span>
                </div>

                <div class="isbn-data">
                    <span class="isbn-data">{%- isbn %}</span>
                </div>

                <div class="notes">
                    <span class="notes-data">{%- notes %}</span>
                </div>

                <div class="publication-links">
                    <span class="download-link">
                        {%- publicationLinks %}
                    </span>
                </div>
            </div>
        </article>
    </script>
